{
    "extends": ["airbnb", "prettier"],
    "plugins": ["@typescript-eslint", "react"],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "import/no-unresolved": 0,
        "react/prop-types": "off",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".jsx", ".tsx"]
            }
        ]
    }
}
